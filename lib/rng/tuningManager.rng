<?xml version="1.0" ?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0"
         datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <start>
    <element name="tuningConfig">
      <zeroOrMore>
        <choice>
	<element name="tuningTable">
	  <attribute name="name" />
	  <optional>
	    <attribute name="alwaysUpdate" />
	  </optional>
	  <optional>
	    <attribute name="target" />
	  </optional>
	  <optional>
	    <attribute name="prefixEnabled" />
	  </optional>
	  <zeroOrMore>
            <choice>
              <element name="comment">
                  <text/>
              </element>
              <element name="internalDependency">
		<attribute name="name" />
              </element>
              <element name="externalDependency">
		<attribute name="name" />
		<optional>
		  <attribute name="dblink" />
		</optional>
		<optional>
		  <attribute name="noTrigger" />
		</optional>
              </element>
              <element name="externalTuningTableDependency">
		<attribute name="name" />
		<optional>
		  <attribute name="dblink" />
		</optional>
              </element>
              <element name="ancillaryTable">
		<attribute name="name" />
              </element>
            </choice>
	  </zeroOrMore>
	  <zeroOrMore>
            <element name="intermediateTable">
              <attribute name="name" />
            </element>
	  </zeroOrMore>
	  <zeroOrMore>
            <element name="unionization">
              <oneOrMore>
		<element name="source">
                  <choice>
                    <attribute name="dblink"/>
                    <element name="query">
                      <text/>
                    </element>
                  </choice>
		</element>
              </oneOrMore>
            </element>
	  </zeroOrMore>
	  <zeroOrMore>
            <element name="sql">
              <text/>
            </element>
	  </zeroOrMore>    
	  <zeroOrMore>
            <element name="perl">
              <text/>
            </element>
	  </zeroOrMore>
	  <zeroOrMore>
            <element name="program">
              <attribute name="commandLine" />
            </element>
	  </zeroOrMore>    
	</element>
	<element name="import">
          <attribute name="file"/>
	</element>
        </choice>
      </zeroOrMore>
    </element>
  </start>
</grammar>
